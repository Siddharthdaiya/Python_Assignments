   "source": [
    "# Write a Python function to check whether a number is perfect or not\n",
    "def perfect_no(n):\n",
    "    sum=0\n",
    "    for i in range(1,n):\n",
    "        if n % i==0:\n",
    "            sum+=i\n",
    "    return sum==n\n",
    "perfect_no(int(input()))"
   ]
