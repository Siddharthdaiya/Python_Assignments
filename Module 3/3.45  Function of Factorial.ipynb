   "source": [
    "# Write a Python function to calculate the factorial of a number (a nonnegative integer)\n",
    "def fact(n):\n",
    "    res=1\n",
    "    for i in range(1,n+1):\n",
    "        res*=i\n",
    "    return res\n",
    "fact(5)"
   ]
